/**
 * @params arr: Array<object>
 * @params valï¼šString | Number  == object.value
 * @return object.label: String
 */
var getLabel = function(arr,val){
  if (!arr) return ''
  var result = ''
  
  for(var i=0;i<arr.length;i++){
    if(arr[i].value == val){
      result = arr[i].label
    }
  }

  return result
}


module.exports  = {
  getLabel: getLabel
}